{
  "name": "tolmachov/yii2-amqp",
  "description": "Yii 2 extension wrapper to communicate with RabbitMQ server via AMQP. Based on php-amqplib/php-amqplib.",
  "keywords": [
    "yii",
    "extension",
    "amqp",
    "RabbitMQ"
  ],
  "homepage": "https://github.com/tolmachov/yii2-amqp",
  "type": "yii-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Vitalii Tolmachov",
      "email": "vetaswind@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "yiisoft/yii2": "^2.0",
    "php-amqplib/php-amqplib": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "tolmachov\\amqp\\": "src/"
    }
  }
}
